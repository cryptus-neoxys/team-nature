// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Donation {
  id          Int      @id @default(autoincrement())
  count       Int
  displayName String
  email       String
  createdAt   DateTime @default(now())
  mobile      String?
  team        String?
  message     String?
}
